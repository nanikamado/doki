import "../../library/prelude.doki"

fix = f:
    (x: f (y: x x y))
    (x: f (y: x x y))

main = _:
    let fib = fix (fib: n: a: b:
        match n with
            0 => a,
            _ => fib (minus n 1) b (plus a b),
        end)
    in let _ = print "> "
    in println (i64_to_string (fib (read_num 0) 0 1))
