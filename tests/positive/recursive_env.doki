print = Str l p: intrinsic_write p l

println = a: let _ = print a
    in print "\n"

data Nil 0
data Cons 2

print_list = l: match l with
        Nil => Nil,
        Cons h t => let _ = println (to_string h)
                in print_list t,
    end

data A 0

to_string = a: match a with
        A => "A",
        f => f Nil,
    end

main = _: let l = Nil
    in let a = A
    in let l = Cons a l
    in let l = Cons (_: to_string a) l
    in print_list l
